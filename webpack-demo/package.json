    "clean-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.6.0",
    "webpackbar": "^5.0.0-3"
